<% include partials/header %>
<div class="blog-content">
  <div id="posts">
    <% for (let i = 0; i < posts.length; i++) { %>
      <div class="post" id="<%= posts[i]._id %>">
        <h3 id="<%= posts[i]._id %>"><%= posts[i].title %></h3>
        <p><%= posts[i].content %></p>
        <p><%= posts[i].post_date %></p>
        <p><%= posts[i].last_update %></p>
        <p><%= posts[i]._id %></p>
      </div>
    <% } %>
  </div>
</div>
<% include partials/footer %>
